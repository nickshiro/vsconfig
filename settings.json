{
  //! https://github.com/nickshiro/vsconfig
  //* VSC update
  "update.mode": "manual",
  "update.showReleaseNotes": true,
  "update.enableWindowsBackgroundUpdates": false,
  /*
     
  */
  //* VSC security
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  /*
     
  */
  //* Extensions
  "extensions.webWorker": true,
  "extensions.autoUpdate": false,
  "extensions.autoCheckUpdates": true,
  "extensions.ignoreRecommendations": true,
  /*
     
  */
  //* Files
  "files.autoSave": "off",
  "files.maxMemoryForLargeFilesMB": 8196,
  "files.autoGuessEncoding": true,
  /*
     
  */
  //* Theme
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Atom Background Modified Italic",
  "workbench.productIconTheme": "bongocat",
  /*
     
  */
  //* Editor
  "editor.lineNumbers": "on",
  "editor.folding": false,
  "editor.stablePeek": false,
  "editor.glyphMargin": false,
  "editor.quickSuggestionsDelay": 0,
  "editor.stickyScroll.enabled": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.bracketPairColorization.enabled": true,
  //? Editor scroll
  "editor.mouseWheelScrollSensitivity": 2,
  "editor.scrollBeyondLastLine": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.horizontalScrollbarSize": 0,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.smoothScrolling": true,
  //? Editor minimap
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  //? Editor paddings
  "editor.padding.top": 2,
  "editor.padding.bottom": 0,
  //? Editor font
  "editor.fontFamily": "Fira Code, monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.fontWeight": "550",
  "editor.letterSpacing": 0,
  "editor.lineHeight": 0,
  //? Editor goto
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  /*
     
  */
  //* Integrated terminal
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.env.windows": {},
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  //? Terminal scroll
  "terminal.integrated.mouseWheelScrollSensitivity": 1,
  "terminal.integrated.smoothScrolling": true,
  //? Terminal font
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontWeight": "normal",
  "terminal.integrated.fontFamily": "Consolas",
  /*
     
  */
  //* Window
  "window.commandCenter": false,
  "window.confirmBeforeClose": "keyboardOnly",
  "window.customMenuBarAltFocus": false,
  "window.density.editorTabHeight": "compact",
  "window.menuBarVisibility": "toggle",
  "window.newWindowDimensions": "inherit",
  "window.openFilesInNewWindow": "off",
  "window.restoreFullscreen": true,
  "window.restoreWindows": "all",
  "window.title": "${rootName}",
  "window.zoomLevel": 0,
  /*
     
  */
  //* Explorer
  "explorer.autoReveal": false,
  "explorer.compactFolders": false,
  "explorer.confirmPasteNative": false,
  "explorer.decorations.badges": false,
  "explorer.decorations.colors": true,
  "explorer.fileNesting.enabled": false,
  //? Confirm
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  /*
     
  */
  //* Workbench
  "workbench.sideBar.location": "right",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.empty.hint": "hidden",
  "workbench.layoutControl.enabled": false,
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "workbench.statusBar.visible": true,
  "workbench.view.alwaysShowHeaderActions": true,
  //? Files tree
  "workbench.tree.indent": 12,
  "workbench.tree.expandMode": "doubleClick",
  "workbench.tree.renderIndentGuides": "none",
  //? Scroll
  "workbench.list.smoothScrolling": true,
  "workbench.list.mouseWheelScrollSensitivity": 2,
  //? File tab config
  "workbench.editor.decorations.badges": false,
  "workbench.editor.decorations.colors": true,
  "workbench.editor.enablePreview": false,
  "workbench.editor.labelFormat": "default",
  "workbench.editor.tabActionLocation": "right",
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.wrapTabs": true,
  /*
   
  */
  //* Goto
  "references.preferredLocation": "view", // Open references in sidebar
  "testing.automaticallyOpenPeekView": "never", // Disable peek view
  /*
     
  */
  //* Breadcrumbs
  "breadcrumbs.enabled": true,
  "breadcrumbs.icons": false,
  "breadcrumbs.symbolPath": "off",
  "breadcrumbs.showKeys": false,
  "breadcrumbs.showArrays": false,
  "breadcrumbs.showEvents": false,
  "breadcrumbs.showFields": false,
  "breadcrumbs.showClasses": false,
  "breadcrumbs.showMethods": false,
  "breadcrumbs.showNumbers": false,
  "breadcrumbs.showObjects": false,
  "breadcrumbs.showModules": false,
  "breadcrumbs.showBooleans": false,
  "breadcrumbs.showPackages": false,
  "breadcrumbs.showFunctions": false,
  "breadcrumbs.showConstants": false,
  "breadcrumbs.showOperators": false,
  "breadcrumbs.showInterfaces": false,
  "breadcrumbs.showNamespaces": false,
  "breadcrumbs.showEnumMembers": false,
  "breadcrumbs.showConstructors": false,
  /*
   
  */
  //* Import coast (extension)
  "importCost.margin": 1,
  "importCost.fontStyle": "italic",
  "importCost.bundleSizeDecoration": "both",
  "importCost.showCalculatingDecoration": true,
  //? Color settings
  "importCost.smallPackageDarkColor": "#98C379",
  "importCost.mediumPackageDarkColor": "#ABB2BF",
  "importCost.largePackageDarkColor": "#ABB2BF",
  /*
   
  */
  //* Inline fold (extension)
  "inlineFold.autoFold": true,
  "inlineFold.togglePerFile": true,
  "inlineFold.maskChar": "•⩊•",
  "inlineFold.unfoldedOpacity": 1,
  "inlineFold.maskColor": "#98C379",
  "inlineFold.regex": "(class|className)=(({(`|))|(['\"`]))(.*?)(\\2|(\\4)})",
  /*
   
  */
  //* Git config
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "never",
  "git.openDiffOnClick": true,
  /*
     
  */
  //* Task manager config (extension)
  "taskExplorer.autoRefresh": true,
  "taskExplorer.showFavoritesButton": false,
  /*
     
  */
  //* Spellchecker (extension)
  "cSpell.enabled": true,
  "cSpell.language": "en",
  "cSpell.words": [],
  "cSpell.enableFiletypes": [
    "blade",
    "jsx",
    "tsx",
    "ts",
    "js",
    "scss",
    "sass",
    "vue"
  ],
  /* 
  
  */
  //* Code runner (extension)
  "python.terminal.executeInFileDir": true,
  "code-runner.runInTerminal": true,
  /* 
  
  */
  //* Todo tree (extension)
  "todo-tree.highlights.enabled": false,
  "todo-tree.highlights.useColourScheme": false,
  "todo-tree.tree.autoRefresh": true,
  "todo-tree.tree.disableCompactFolders": true,
  "todo-tree.tree.expanded": true,
  "todo-tree.tree.flat": true,
  "todo-tree.tree.scanAtStartup": true,
  /* 
  
  */
  //* Format
  "editor.formatOnSave": true,
  "editor.formatOnPaste": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  //? Eslint (extension)
  "eslint.run": "onSave",
  "eslint.format.enable": true,
  //? Prettier (extension)
  "prettier.useEditorConfig": true,
  "prettier.requireConfig": true,
  "prettier.configPath": ".prettierrc",
  /*
     
  */
  //* Emmet (Shortcut for html tags)
  "emmet.triggerExpansionOnTab": true,
  "emmet.optimizeStylesheetParsing": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact",
    "blade": "html"
  },
  /*
     
  */
  //* Languages config
  //? TS & JS
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  //? React
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  //? Css
  "[css]": {
    "editor.tabSize": 2,
    "editor.colorDecorators": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[sass]": {
    "editor.tabSize": 2,
    "editor.colorDecorators": false,
    "editor.defaultFormatter": "syler.sass-indented"
  },
  "[postcss]": {},
  //? Html
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  //? Json
  "[json, jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.tabSize": 2
  },
  //? Python
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.tabSize": 4
  },
  //? Prisma
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  //? Markdown
  "workbench.editorAssociations": {
    "*.md": "cweijan.vscode-markdown-enhanced",
    "{git,gitlens}:/**/*.{md,csv,svg}": "default"
  },
  "vscode-office.editorTheme": "Auto",
  /*
     
  */
  //* Telemetry
  "telemetry.telemetryLevel": "off",
  "redhat.telemetry.enabled": false,
  "postman.telemetry.enabled": false,
  /* 
  
  */
  //* Remote hosts
  "remote.SSH.remotePlatform": {
    "147.45.137.180": "linux",
    "217.25.90.240": "linux"
  }
}
